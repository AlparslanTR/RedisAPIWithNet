@model HashSet<string>
@{
    ViewData["Title"] = "Index";
}

<form asp-controller="HashSetTypes" asp-action="Add" method="post" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <button type="submit">Kaydet</button>
</form>

    @if (Model.Count > 0)
    {
        @foreach (var item in Model)
        {
                <p>@item</p>
        }
    }
    else
    {
        <p>Redis Sunucusunda veri yok.</p>
    }



